{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "sourceMaps": true,
  "retainLines": true,
  "ignore": [
    "node_modules",
    ".dist"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./server"]
    }]
  ]
}